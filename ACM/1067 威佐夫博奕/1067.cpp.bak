#include <stdio.h>
#include <math.h>

int main(int argc, char *argv[])
{
	__int64 a,b;
	while(scanf("%I64d %I64d",&a,&b)==2)
	{
		double x=(sqrt((float)5)+1)/2;

		__int64 k=b-a;

		if(k<0)
			k=-k;

		__int64 tmp=(__int64)(k*x+k);
		__int64 tmp2;

		if(a<b)
			tmp2=b;
		else
			tmp2=a;

		if(tmp==tmp2)
			printf("0\n");
		else
			printf("1\n");
	}
	
	return 0;
}
